        .balloon:before {
            content: "";
            position: absolute;
            width: 2px;
            height: 100px;
            background: rgba(255, 255, 255, 0.5);
            top: 70px;
            left: 50%;
            transform: translateX(-50%);
        }
        
        .balloon:nth-child(1) {
            background: #ff6b6b;
            left: 10%;
            animation-delay: 0s;
        }
        
        .balloon:nth-child(2) {
            background: #4ecdc4;
            left: 25%;
            animation-delay: 2s;
        }
        
        .balloon:nth-child(3) {
            background: #ffd166;
            left: 40%;
            animation-delay: 4s;
        }
        
        .balloon:nth-child(4) {
            background: #118ab2;
            left: 55%;
            animation-delay: 1s;
        }
        
        .balloon:nth-child(5) {
            background: #ef476f;
            left: 70%;
            animation-delay: 3s;
        }
        
        .balloon:nth-child(6) {
            background: #06d6a0;
            left: 85%;
            animation-delay: 5s;
        }
        
        @keyframes float {
            0% {
                bottom: -100px;
                transform: translateX(0);
            }
